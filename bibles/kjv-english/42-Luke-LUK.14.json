"items":[{"name":"verse-span","type":"tag","attrs":{"class":"verse-span","verseId":"LUK.14.8","verseOrgIds":["LUK.14.8"]},"items":[{"text":"When thou art bidden of any ","type":"text","attrs":{"verseId":"LUK.14.8","verseOrgIds":["LUK.14.8"]}}]},{"name":"char","type":"tag","attrs":{"style":"add"},"items":[{"name":"verse-span","type":"tag","attrs":{"class":"verse-span","verseId":"LUK.14.8","verseOrgIds":["LUK.14.8"]},"items":[{"text":"man","type":"text","attrs":{"verseId":"LUK.14.8","verseOrgIds":["LUK.14.8"]}}]}]},{"name":"verse-span","type":"tag","attrs":{"class":"verse-span","verseId":"LUK.14.8","verseOrgIds":["LUK.14.8"]},"items":[{"text":" to a wedding, sit not down in the highest room; lest a more honourable man than thou be bidden of him;","type":"text","attrs":{"verseId":"LUK.14.8","verseOrgIds":["LUK.14.8"]}}]}]},{"name":"verse-span","type":"tag","attrs":{"class":"verse-span","verseId":"LUK.14.8","verseOrgIds":["LUK.14.8"]},"items":[{"text":" ","type":"text","attrs":{"verseId":"LUK.14.8","verseOrgIds":["LUK.14.8"]}}]},{"name":"verse-span","type":"tag","attrs":{"class":"verse-span","verseId":"LUK.14.9","verseOrgIds":["LUK.14.9"]},"items":[{"name":"verse","type":"tag","attrs":{"number":"9","style":"v","sid":"LUK 14:9"},"items":[{"text":"9","type":"text"}]}]},{"name":"char","type":"tag","attrs":{"style":"wj"},"items":[{"name":"verse-span","type":"tag","attrs":{"class":"verse-span","verseId":"LUK.14.9","verseOrgIds":["LUK.14.9"]},"items":[{"text":"And he that bade thee and him come and say to thee, Give this man place; and thou begin with shame to take the lowest room.","type":"text","attrs":{"verseId":"LUK.14.9","verseOrgIds":["LUK.14.9"]}}]}]},{"name":"verse-span","type":"tag","attrs":{"class":"verse-span","verseId":"LUK.14.9","verseOrgIds":["LUK.14.9"]},"items":[{"text":" ","type":"text","attrs":{"verseId":"LUK.14.9","verseOrgIds":["LUK.14.9"]}}]},{"name":"verse-span","type":"tag","attrs":{"class":"verse-span","verseId":"LUK.14.10","verseOrgIds":["LUK.14.10"]}